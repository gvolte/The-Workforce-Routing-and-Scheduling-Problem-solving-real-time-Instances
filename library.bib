journal = {European Journal of Operational Research},
keywords = {Health care,Heuristics,Home care,Modelling,Optimization,Practice of OR,Staff planning},
number = {3},
pages = {962--976},
title = {{Laps Care-an operational system for staff planning of home care}},
volume = {171},
year = {2006}
