@article{Tsitsiklis1992,
abstract = {Consider a complete directed graph in which each arc has a given length. There is a set of jobs, each job i located at some node of the graph, with an associated processing time hi, and whose execution has to start within a presepecified time window [ri, di]. We have a single server that can move on the arcs of the graph, at unit speed, and that has to execute all of the jobs within their respective time windows. We consider the following two problems: (a) minimize the time by which all jobs are executed (traveling salesman problem) and (b) minimize the sum of the waiting times of the jobs (traveling repairman problem). We focus on the following two special cases: (a) The jobs are located on a line and (b) the number of nodes of the graph is bounded by some integer constant B. Furthermore, we consider in detail the special cases where (a) all of the processing times are 0, (b) all of the release times ri are 0, and (c) all of the deadlines di are infinite. For many of the resulting problem combinations, we settle their complexity either by establishing NP-completeness or by presenting polynomial (or pseudopolynomial) time algorithms. Finally, we derive algorithms for the case where, for any time t, the number of jobs that can be executed at that time is bounded.},
author = {Tsitsiklis, John N.},
doi = {10.1002/net.3230220305},
file = {:C$\backslash$:/Users/Gabriel/Downloads/biblio/complexite/Tsitsiklis1992.pdf:pdf},
issn = {10970037},
journal = {Networks},
number = {3},
pages = {263--282},
title = {{Special cases of traveling salesman and repairman problems with time windows}},
volume = {22},
year = {1992}
}
